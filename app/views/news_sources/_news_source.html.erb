<div class="col-md-4">
  <h3 style="margin-top: 1em">
    <span class='newsSourceTitle'><%= news_source.name %></span>
  </h3>
  <div class="vstack gap-1">
    <%= news_source.stories.each do |story| %>
      <%= render :partial=>'story_row', :locals=>{:story=>story} %>
    <% end %>
  </div>
</div>